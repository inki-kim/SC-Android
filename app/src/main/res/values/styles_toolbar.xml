<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Toolbar" parent="Base.V7.Widget.AppCompat.Toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">?android:attr/actionBarSize</item>
        <item name="android:background">@color/alphaPercent0</item>
    </style>

    <style name="Toolbar.Transparent">
        <item name="android:background">@color/alphaPercent0</item>
    </style>

    <style name="Toolbar.Grey">
        <item name="android:background">@color/grey3</item>
    </style>
</resources>